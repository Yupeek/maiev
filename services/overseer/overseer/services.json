{
  "services": [
    {
      "name": "{dirname}",
      "extends": "docker",
      "context": {
        "extra_link": "--link=overseer_mongodb"
      }
    },
    {
      "name": "overseer_mongodb",
      "extends": "docker-db",
      "retry": 2,
      "context": {
        "engine_name": "mongodb"
      }
    }
  ]
}