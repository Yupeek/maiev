{
  "services": [
    {
      "name": "{dirname}",
      "exec": [
        "/usr/bin/docker",
        "run",
        "-i",
        "--rm",
        "--network={networkname}",
        "-p", "5000:5000",
        "--link=scaler_docker:scaler_docker",
        "--name={docker_name}",
        "{service_name}:dev"
      ],
      "execStop": [
        "/usr/bin/docker",
        "stop",
        "{docker_name}"
      ],
      "type": "script",
      "waitUntil": 2500
    }
  ],
  "context": {
    "extra_volume": "-v=/var/run/docker.sock:/var/run/docker.sock"
  }
}