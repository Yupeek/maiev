{
  "services": [
    {
      "name": "{dirname}",
      "extends": "docker",
      "require": [
        "super()",
        "registry_docker"
      ]
    }
  ],
  "context": {
    "extra_volume": "-v=/var/run/docker.sock:/var/run/docker.sock",
    "extra_env": ["-e", "NAMEKO_LOG_LEVEL=INFO", "-e", "CONSOLE_LOG_FILTERS=[filtergetroot]"]
  },
  "discover": []
}