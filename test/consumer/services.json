{
  "services": [
    {
      "name": "{dirname}",
      "extends": "docker",
      "type": "test",
      "context": {

      },
      "require": [
        "super()",
        "producer"
      ],
      "extraData": [
        {
          "name": "trigger other queries",
          "exec": ["/bin/bash", "-c",
            "sleep 1 && echo \"http://$(docker inspect {dirname} | jq -r '.[].NetworkSettings.Networks.\"{networkname}\".IPAddress'):8000/\""
          ],
          "type": "link"
        }
      ]
    }
  ]
}